@{
}


<table class="table table-bordered">
    <tr>
        <td>ShipperId</td>
        <td>CompanyName</td>
        <td>Phone</td>



    </tr>

    @foreach (var s in ViewBag.ListOfShippers)
    {
        <tr>
            <td>@s.ShipperId</td>
            <td>@s.CompanyName</td>
            <td>@s.Phone</td>
        </tr>

    }




</table>